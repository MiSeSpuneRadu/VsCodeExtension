{
  "name": "open-all-terminals",
  "displayName": "Open All Terminals",
  "description": "Open all microservice terminals",
  "version": "0.0.1",
  "engines": {
      "vscode": "^1.60.0"
  },
  "categories": [
      "Other"
  ],
  "activationEvents": [
      "onCommand:extension.openAllTerminals"
  ],
  "main": "./out/extension.js",
  "contributes": {
      "commands": [
          {
              "command": "extension.openAllTerminals",
              "title": "Open All Terminals"
          }
      ]
  },
  "scripts": {
      "vscode:prepublish": "npm run compile",
      "compile": "tsc -p ./",
      "watch": "tsc -watch -p ./",
      "pretest": "npm run compile && npm run lint",
      "lint": "eslint src --ext ts",
      "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
      "@types/vscode": "^1.60.0",
      "@types/glob": "^7.1.3",
      "@types/mocha": "^9.0.0",
      "@types/node": "16.x",
      "eslint": "^7.32.0",
      "glob": "^7.1.7",
      "mocha": "^9.0.0",
      "typescript": "^4.4.2",
      "@vscode/test-electron": "^2.0.0"
  }
}
